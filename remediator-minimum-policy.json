{
	"Version": "2012-10-17",
	"Statement": [
		{
			"Effect": "Allow",
			"Action": [
				"s3:ListAllMyBuckets",
				"s3:GetBucketAcl",
				"s3:PutBucketAcl",
				"s3:PutBucketPublicAccessBlock"
			],
			"Resource": "*"
		},
		{
			"Effect": "Allow",
			"Action": [
				"ec2:DescribeSecurityGroups",
				"ec2:RevokeSecurityGroupIngress"
			],
			"Resource": "*"
		},
		{
			"Effect": "Allow",
			"Action": [
				"iam:ListUsers",
				"iam:ListAccessKeys",
				"iam:UpdateAccessKey",
				"iam:ListMFADevices"
			],
			"Resource": "*"
		}
	]
}
